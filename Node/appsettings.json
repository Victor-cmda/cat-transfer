{
  "Node": {
    "NodeName": "CatTransferNode",
    "Network": {
      "Host": "localhost",
      "Port": 8080,
      "RemotePort": 8081,
      "SeedNodes": [],
      "MaxConnections": 100,
      "ConnectionTimeout": "00:00:30",
      "HeartbeatInterval": "00:00:10"
    },
    "Storage": {
      "DataDirectory": "%APPDATA%\\CatTransfer",
      "TempDirectory": "%TEMP%\\CatTransfer",
      "MaxFileSize": 10737418240,
      "MaxConcurrentTransfers": 10
    },
    "Transfer": {
      "DefaultChunkSize": 65536,
      "MaxChunkSize": 1048576,
      "MaxConcurrentChunks": 8,
      "TransferTimeout": "00:30:00",
      "ChunkTimeout": "00:00:30",
      "MaxRetries": 3
    },
    "Logging": {
      "LogLevel": "Information",
      "EnableFileLogging": true,
      "EnableConsoleLogging": true,
      "MaxLogFiles": 30
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Akka": "Information"
    }
  }
}
